DROP DATABASE IF EXISTS cars;
CREATE DATABASE CARS DEFAULT CHARACTER SET utf8

USE cars

CREATE TABLE CARADVERTS (
ID VARCHAR(64) NOT NULL PRIMARY KEY,
TITLE VARCHAR(512) NOT NULL,
FUEL VARCHAR(256) NOT NULL,
PRICE INT NOT NULL,
NEWCAR TINYINT(1) NOT NULL,
MILEAGE INT,
FIRSTRREGISTRATION VARCHAR(20)
);

